    "isomorphic-unfetch": "3.0.0",
    "es6-promise": "4.2.8",
    "js-cookie": "2.2.1",
    "qs": "6.9.1",
    "lodash": "4.17.15",
    "next-page-transitions": "1.0.0-beta.2",
    "nprogress": "0.2.0",
    "postcss": "7.0.27",
